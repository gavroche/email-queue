{
    "name": "sinergi/emailqueue",
    "type": "library",
    "description": "Email Queue powered by Gearman",
    "keywords": ["Email", "Queue", "Gearman"],
    "homepage": "https://github.com/sinergi/emailqueue",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.*",
        "doctrine/orm": "2.*",
        "psr/log": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "suggest": {
        "ext-gearman": "This library needs Gearman"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\EmailQueue\\": "src/EmailQueue"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\EmailQueue\\Tests\\": ["tests/EmailQueue/Tests", "tests/EmailQueue/Tests/_includes"]
        }
    }
}
