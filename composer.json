{
    "name": "smart/emailqueue",
    "type": "library",
    "description": "Email Queue powered by Gearman",
    "keywords": ["Email", "Queue", "Gearman"],
    "homepage": "https://github.com/smart-io/emailqueue",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2",
        "doctrine/orm": "~2",
        "cocur/slugify": "~1",
        "sinergi/gearman": "<1",
        "sinergi/token": "<1",
        "psr/log": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "znarkus/postmark": "dev-master"
    },
    "suggest": {
        "ext-gearman": "This library needs Gearman"
    },
    "autoload": {
        "psr-4": {
            "Smart\\EmailQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smart\\EmailQueue\\Tests\\": ["tests/EmailQueue/Tests"]
        }
    }
}
